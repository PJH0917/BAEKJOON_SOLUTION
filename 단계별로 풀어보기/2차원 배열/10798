#include <stdio.h>

int main() {
    char arr[5][15] = {NULL};
    for (int i = 0; i < 5; i++) {
        scanf("%s", arr[i],sizeof(arr));
    }
    for (int j = 0; j < 15; j++) {
        for (int i = 0; i < 5; i++) {
            if (arr[i][j] == NULL) {
                continue;
            }
            else
                printf("%c",arr[i][j]);
        }
    }
    return 0;
}
