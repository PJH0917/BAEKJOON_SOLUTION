#include <stdio.h>

int main() {
    int MAX = 0, location, arr[9];
    for (int i = 0; i < 9; i++) {
        scanf("%d", &arr[i]);  // \n 제거
        if (arr[i] >= MAX) {
            MAX = arr[i];
            location = i;
            
        }
    }

    printf("%d\n", MAX);
    printf("%d",location+1 );

    return 0;
}
