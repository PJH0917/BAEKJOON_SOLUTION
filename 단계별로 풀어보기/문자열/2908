#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
    int num1, num2;
    scanf("%d %d", &num1, &num2);   
    int reverse_num1 = (num1 % 10) * 100 + ((num1 / 10) % 10) * 10 + num1 / 100;
    int reverse_num2 = (num2 % 10) * 100 + ((num2 / 10) % 10) * 10 + num2 / 100;
    if (reverse_num1 > reverse_num2) {
        printf("%d\n", reverse_num1);
    }
    else {
        printf("%d\n", reverse_num2);
    }
    return 0;
}
